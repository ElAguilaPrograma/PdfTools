{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "pdftoolsuiclient",
  "version": "0.1.0",
  "identifier": "com.jemun.pdftoolsuiclient",
  "build": {
    "beforeDevCommand": "npm run start",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist/pdftoolsuiclient/browser"
  },
  "app": {
    "windows": [
      {
        "title": "Pdf Tools",
        "width": 1100,
        "height": 615,
        "minWidth": 400,
        "minHeight": 300,
        "hiddenTitle": false,
        "visible": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {
    "fs": {},
    "opener": {}
  }
}